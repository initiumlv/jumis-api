{
  "name": "initiumlv/jumis-api",
  "description": "PHP library for integrating with Tildes Jumis ERP system's XML-based API",
  "version": "1.0.1",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Initium\\Jumis\\Api\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Initium\\Jumis\\Api\\JumisApiServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^12.0",
    "guzzlehttp/guzzle": "^7.0",
    "ext-simplexml": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
