    "keywords": ["laravel", "jumis", "api"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Initium\\Jumis\\Api\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kristians Jaunzems",
            "email": "kristians@initium.lv"
        }
    ],
    "minimum-stability": "stable",
