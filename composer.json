{
    "name": "initiumlv/jumis-api",
    "description": "PHP library for integrating with Tildes Jumis ERP system's XML-based API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Initium\\Jumis\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Initium\\Jumis\\Api\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Initium\\Jumis\\Api\\JumisApiServiceProvider"
            ],
            "aliases": {
                "JumisApi": "Initium\\Jumis\\Api\\Facades\\JumisApi"
            }
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
